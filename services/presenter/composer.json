{
    "name": "noframework/presenter",
    "version": "0.1",
    "description": "Presenter Service",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Fux",
            "email": "it@noframework.io"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-mbstring": "*",
        "ext-curl": "*",
        "ext-fileinfo": "*"
    },
    "autoload": {
        "psr-4": {
            "noframework\\Presenter\\" : "src"
        }
    },
    "require-dev" : {
        "phpunit/phpunit": "^9.3"
    },
    "autoload-dev": {
        "psr-4": {
            "noframework\\Test\\Presenter\\": "tests"
        }
    },
    "minimum-stability": "dev"
}
